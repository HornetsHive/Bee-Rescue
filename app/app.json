{
  "expo": {
    "name": "BeeRescue",
    "slug": "BeeRescue",
    "version": "1.0.0",
    "orientation": "default",
    "packagerOpts": {
      "sourceExts": ["js", "json", "ts", "tsx", "jsx", "vue"],
      "config": "metro.config.js"
    },

    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF",
        "useNextNotificationsApi": true,
        "package": "com.SABA.BeeRescue",
        "permissions": [
          "READ_EXTERNAL_STORAGE",
          "WRITE_EXTERNAL_STORAGE",
          "NOTIFICATIONS"
        ],
        "googleServicesFile": "./google-services.json",
        "versionCode": 1
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB2UXCGw1CCXq9GJVsyu-j6W75q2PGd5R8"
        }
      }
    },
    "web": {}
  }
}
